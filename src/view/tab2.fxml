<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.quantityController">
    <children>
        <TableView fx:id="table" layoutX="6.0" layoutY="14.0" prefHeight="400.0" prefWidth="400.0">
            <columns>
                <TableColumn fx:id="c1" prefWidth="117.0" text="Пиццерия"/>
                <TableColumn fx:id="c2" prefWidth="122.0" text="Количество пицц"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <Button layoutX="521.0" layoutY="212.0" mnemonicParsing="false" onAction="#Reload" text="Обновить данные"/>
        <Button layoutX="680.0" layoutY="398.0" mnemonicParsing="false" onAction="#Upload" text="Upload"/>
        <Button layoutX="626.0" layoutY="398.0" mnemonicParsing="false" onAction="#Save" text="Save"/>
    </children>
</AnchorPane>
