<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.pizzaTypesController">
    <children>
        <TableView fx:id="table" layoutX="6.0" layoutY="14.0" prefHeight="378.0" prefWidth="420.0">
            <columns>
                <TableColumn fx:id="c1" prefWidth="117.0" text="Пиццерия"/>
                <TableColumn fx:id="c2" prefWidth="122.0" text="Пицца"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <Button layoutX="497.0" layoutY="200.0" mnemonicParsing="false" onAction="#addButton" text="Add"
                AnchorPane.topAnchor="200.0"/>
        <TextField fx:id="textField1" layoutX="443.0" layoutY="14.0" prefHeight="27.0" prefWidth="150.0"
                   promptText="restaurant name" AnchorPane.topAnchor="50.0"/>
        <TextField fx:id="textField2" layoutX="443.0" layoutY="98.0" prefHeight="27.0" prefWidth="150.0"
                   promptText="pizza name" AnchorPane.topAnchor="100.0"/>
        <TextField fx:id="textField3" layoutX="443.0" layoutY="146.0" prefHeight="27.0" prefWidth="150.0"
                   promptText="weight" AnchorPane.topAnchor="150.0"/>
    </children>
</AnchorPane>
