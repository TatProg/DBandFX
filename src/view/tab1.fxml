<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.pizzaTypesController">
    <children>
        <TableView fx:id="table" layoutX="6.0" layoutY="14.0" prefHeight="378.0" prefWidth="420.0">
            <columns>
                <TableColumn fx:id="c1" prefWidth="117.0" text="Пиццерия"/>
                <TableColumn fx:id="c2" prefWidth="122.0" text="Пицца"/>
                <TableColumn fx:id="c3" prefWidth="75.0" text="Цена"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <Button layoutX="475.0" layoutY="279.0" mnemonicParsing="false" onAction="#saveClick" prefHeight="31.0"
                prefWidth="95.0" text="Сохранить"/>
        <Button layoutX="475.0" layoutY="327.0" mnemonicParsing="false" onAction="#loadClick" prefHeight="31.0"
                prefWidth="95.0" text="Загрузить"/>
        <Button layoutX="475.0" layoutY="53.0" mnemonicParsing="false" onAction="#addClick" prefHeight="31.0"
                prefWidth="95.0" text="Добавить"/>
        <Button layoutX="475.0" layoutY="109.0" mnemonicParsing="false" onAction="#editClick" prefHeight="31.0"
                prefWidth="95.0" text="Изменить"/>
        <Button layoutX="475.0" layoutY="163.0" mnemonicParsing="false" onAction="#deleteClick" prefHeight="31.0"
                prefWidth="95.0" text="Удалить"/>
    </children>
</AnchorPane>
